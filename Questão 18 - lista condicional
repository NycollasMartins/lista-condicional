#include <stdio.h>
int main() {
	int num;
 
	printf("Informe um numero: ");
	scanf("%d", &num);

	if ((num%3 == 0) || (num%5 == 0))
	{
	printf("O numero eh divisivel por 3 ou 5, mas nao por ambos.\n");
	}
	else if ((num%3 == 0) && (num%5 == 0))
	{
	printf("O numero eh divisivel por 3 e 5.\n");
	}
	else
	{
	printf("O numero nao eh divisivel por 3 ou 5.\n");
	}
   
return 0;
}
