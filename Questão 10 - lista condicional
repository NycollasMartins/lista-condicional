#include <stdio.h>

int main() {
    float altura;
    float peso;

    printf("Altura: ");
    scanf("%f", &altura);
    printf("Peso: ");
    scanf("%f", &peso);

    if (altura >= 1.20 && altura <= 1.70) {
        if (peso <= 60) {
            printf("B");
        } else if (peso >= 60 && peso <= 90) {
            printf("E");
        } else {
            printf("H");
        }
    } else {
        if (peso <= 60) {
            printf("C");
        } else if (peso >= 60 && peso <= 90) {
            printf("F");
        } else {
            printf("I");
        }
    }

    return 0;
}
