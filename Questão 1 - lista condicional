#include <stdio.h>

int main () {
	 int num1, num2;
	 
	printf("Programa que leia dois numeros e mostre o maior\n\n");
	 
	printf("Digite o primeiro numero: ");
	scanf("%d", &num1);
	printf("\n");
	 
	printf("Digite o segundo numero: ");
	scanf("%d", &num2);
	printf("\n");
	 
	if(num1 > num2)
	 
		printf("O maior numero eh %d", num1);
	else 
	
	if(num2 > num1)
		printf("O maior numero eh %d", num2);
	
	printf("\n\n");
	printf("################# Fim #################");
	
}
